{
  "manifest_version": 2,
  "name": "Ynet tools",
  "version": "0.2",  
  "permissions": [
    "storage"
 ],
    "options_page": "options.html",

   "content_scripts": [
	    {
	        "matches": ["http://*.ynet.co.il/*","http://*.calcalist.co.il/*","https://*.ynet.co.il/*","https://*.calcalist.co.il/*"],
	        "css": ["ynetExtension.css"],
	        "js": ["jquery-1.11.1.min.js","content.js"]
	    }
	],
	"icons": { "16": "icons/16.png",
           "48": "icons/48.png",
          "128": "icons/128.png" 
      }
}